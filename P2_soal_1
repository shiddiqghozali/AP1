#include <iostream>
#include <string>

using namespace std;

int main()
{
    cout <<"==================DATA DIRI==================\n";
    cout <<endl<<'\n';
    string nama;
    nama = "Abdullah";

    int usia;
    usia = 64;

    float tinggi;
    tinggi = 173.64;

    string hobi;
    hobi = "main bola, nulis, ngoding";

    cout <<"nama = "<<nama<<'\n';

    cout <<"usia = "<<usia<<'\n';

    cout <<"tinggi = "<<tinggi<<'\n';

    cout <<"hobi = "<<hobi<<'\n';
}
